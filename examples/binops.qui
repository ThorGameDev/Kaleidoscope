def i8 operator >> 100 (i8 left, i8 right) { 
    i8 shiftright = 1; 
    for (i8 i = 0; i < right; i = i + 1 ){
        shiftright = shiftright * 2;
    };
    i8 retval = left / shiftright;
    retval
}

def i8 operator << 100 (i8 left, i8 right) { 
    i8 retval = left; 
    for (i8 i = 0; i < right; i = i + 1 ){
        retval = retval * 2;
    };
    retval
}

def i8 operator @ (i8 input){
    input * input
}

